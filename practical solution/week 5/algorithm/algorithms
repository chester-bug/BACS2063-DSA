package algorithm;

/**
 *
 * @author Administrator
 */
public class algorithms {
    
    public static int algorithmA(int n){
        int sum = 0;
        for (int i = 1; i <= n; ++i){
            sum += i;
        }
        return sum;
    }
    
    public static int algorithmB(int n){
        int sum = 0;
        
        for (int i = 1; i <= n; ++i){
            for (int j = 1; j <= i; ++j){
                sum += i;
            }
        }
        return sum;
    }
    
    public static int algorithmC (int n){
        return n * (n + 1) / 2;
    }
    
}
